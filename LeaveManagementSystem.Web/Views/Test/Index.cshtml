@model TestViewModel
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    ViewData["Title"] = "Test View";
}

<h1>Hello from test controller</h1>
<h2>Your name is: @Model.Name</h2>

@if(Model.DateOfBirth.HasValue)
{
    <h3>Your date of birth is: @Model.DateOfBirth.Value.ToString("d")</h3>
}
else
{
    <h3>Date of birth is not provided.</h3>
}
